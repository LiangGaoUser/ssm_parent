<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhongruan.dao.ISsdDao" >
    <select id="findAllSsdInformation" resultType="com.zhongruan.bean.Ssd">
       select*from ssd_table
   </select>

    <select id="findSelectSsd" parameterType="com.zhongruan.bean.Ssd" resultType="com.zhongruan.bean.Ssd">
       select*from ssd_table where ssd_id=#{ssd_id}
   </select>
</mapper>